      "id": "7316f674-2789-4618-a021-eae95f9eb712",
      "name": "FAL LoRA Request",
      "type": "n8n-nodes-base.httpRequest",
      "position": [
        380,
        240
      ],
      "typeVersion": 4.2,
      "credentials": {
